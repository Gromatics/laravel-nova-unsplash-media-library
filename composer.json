{
    "name": "gromatics/laravel-nova-unsplash-media-library",
    "description": "Search and add Unsplash images to media library in Laravel Nova",
    "authors": [
        {
            "name": "Guido Rus"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "unsplash",
        "media library",
        "spatie"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "spatie/laravel-medialibrary": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Gromatics\\LaravelNovaUnsplashMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gromatics\\LaravelNovaUnsplashMediaLibrary\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
